      <Row>
        <Cell>
          <Data ss:Type="String">loc_15373294129891630394</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Restore From Backup</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Відновити з резервної копії</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String"/>
        </Cell>
